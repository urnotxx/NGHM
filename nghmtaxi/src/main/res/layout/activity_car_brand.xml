<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zzx.haoniu.nghmtaxi.activity.CarBrandActivity">

    <LinearLayout
        android:id="@+id/activity_car_brand"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include layout="@layout/common_navigation" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ListView
                android:id="@+id/lvBrandCB"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:background="@color/colorWhite"
                android:divider="@color/transparent"
                android:dividerHeight="0dip"
                android:scrollbars="none"></ListView>

            <ListView
                android:id="@+id/lvModelCB"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:background="@color/colorGrayM"
                android:divider="@color/transparent"
                android:dividerHeight="0dip"></ListView>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
